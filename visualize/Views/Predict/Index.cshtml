@model Tuple<int, string>[]
@{
    ViewData["Title"] = "Prediction";
}
<div id="predictIndex" class="container">
    <h1 class="display-3 text-center">Predictions Results</h1>
    <a class="btn btn-outline-dark mb-3" href='@Url.Action("Index", "Train")'>
        <i class="fas fa-arrow-left"></i> Training Results
    </a>
    <table id="predictionDatatable" class="table table-striped table-bordered mt-3 mb-5" style="width:100%">
        <thead>
            <tr>
                <th>Index</th>
                <th>House</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var result in Model)
            {
                <tr>
                    <td>@result.Item1</td>
                    <td>@result.Item2</td>
                </tr>
            }
        </tbody>
    </table>
</div>